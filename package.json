    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.14",
    "browserslist": "^4.22.3",
    "electron": "^35.1.1",
    "electron-builder": "^26.0.12",
