    "@reduxjs/toolkit": "^1.9.7",
    "@sindresorhus/transliterate": "^1.6.0",
    "bindings": "^1.5.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "electron-log": "^5.3.3",
    "electron-settings": "^4.0.4",
    "electron-shortcuts": "^0.4.0",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "ffbinaries": "^1.1.5",
    "file-type": "^20.4.1",
