    "framer-motion": "^11.0.3",
    "jsdom": "^22.1.0",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "nanoid": "^3.3.7",
    "node-addon-api": "^8.3.1",
    "patch-package": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-transition-group": "^4.4.5",
    "redux": "^4.2.1",
    "sonner": "^1.4.3",
    "sqlite": "^5.1.1",
    "sqlite3": "5.1.6",
    "tasklist": "^5.0.0"
